    (
        'install -m 0755 '
        '        %(builddir)s/jupyterlab_favorites/jupyterlab_favorites-3.0.1/update_favorites_json '
        '        %(installdir)s/bin/ '
    ),

    #   tour-state on/off is saved in workspace-file in ~/.jupyter/lab/workspaces/
